{
	"extends": "@veloi.me/typescript-config/snippets.json",
	"compilerOptions": {
		"outDir": "dist",
		"allowImportingTsExtensions": true,
		"module": "ESNext",
		"target": "ESNext",
		"esModuleInterop": true,
		"moduleResolution": "bundler",
		"jsx": "react-jsx",
		"jsxImportSource": "preact",
		"noEmit": true
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules",
		"dist"
	]
}
